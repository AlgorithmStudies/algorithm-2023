import java.util.*;
//1,3,1/1,1,3//상근이가 먼저하므로 홀수/ 두번째하므로 짝수면 창근
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        String answer = "";

        if(n%2 == 0) 
            answer+="CY";
        else 
            answer+="SK";

        System.out.println(answer);

    }
}
